filter(activeItem => activeItem.isChecked === true);\n      }\n      case 'completed': {\n        return items.filter(completedItem => completedItem.isChecked !== true);\n      }\n      default: {\n        return items;\n      }\n    }\n  };\n\n  return (\n    <section className=\"main\">\n      <input\n        id=\"toggle-all\"\n        className=\"toggle-all\"\n        type=\"checkbox\"\n        checked={isAllChecked()}\n        onChange={() => changeAllChecked(isAllChecked())}\n      />\n      <label htmlFor=\"toggle-all\">Mark all as complete</label>\n      <ul className=\"todo-list\">\n        {filter(data, current).map(item => (\n          <TodoItem data={item} key={v4()} />\n        ))}\n      </ul>\n    </section>\n  );\n};\nexport default MainArea;\n"},"lineCount":null}},"hash":"07fe824b0e12c29ddb0452e092e6c2de","cacheData":{"env":{}}}