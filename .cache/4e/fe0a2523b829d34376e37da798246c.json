\n    default: {\n      return items;\n    }\n  }\n};\n\nconst MainArea = () => {\n  const { data, isAllChecked, changeAllChecked, current } = useContext(Context);\n  if (!data.length) {\n    return null;\n  }\n\n  return (\n    <section className=\"main\">\n      <input\n        id=\"toggle-all\"\n        className=\"toggle-all\"\n        type=\"checkbox\"\n        checked={isAllChecked()}\n        onChange={() => changeAllChecked(isAllChecked())}\n      />\n      <label htmlFor=\"toggle-all\">Mark all as complete</label>\n      <ul className=\"todo-list\">\n        {filter(data, current).map(item => (\n          <TodoItem data={item} key={v4()} />\n        ))}\n      </ul>\n    </section>\n  );\n};\nexport default MainArea;\n"},"lineCount":null}},"hash":"587a7c5d4459f3ec15729a859dcc7a0e","cacheData":{"env":{}}}